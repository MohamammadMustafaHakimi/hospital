<h1>This is the index file and lists all the doctors</h1>
<ul>
  <% @doctors.each do |doctor| %>
    <li>
      <%= link_to "#{doctor.title}.#{doctor.first_name} #{doctor.last_name.capitalize}", doctor_path(doctor) %>
    </li>
    <%#="#{doctor.first_name} #{doctor.last_name}"%>
  <% end %>
</ul>
<%= link_to "Add new Doctor", new_doctor_path%>
